            width: 100% !important;
        }

        form.form-search:before {
            display: block;
            width: 14px;
            height: 14px;
            content: "\e003";
            font-family: 'Glyphicons Halflings';
            background-position: -48px 0;
            position: absolute;
            top: 8px;
            left: 8px;
            opacity: .5;
            z-index: 1000;
        }

        form.form-search {
            position: relative;
        }

        .vira-card .vira-card-header {
            margin-bottom: 45px;
        }

            .vira-card .vira-card-header .card-icon {
                border: 1px solid #00dce8;
                border-radius: 100%;
                display: inline-block;
                height: 120px;
                width: 120px;
                line-height: 120px;
                position: relative;
            }

                .vira-card .vira-card-header .card-icon:after {
                    transition: all 0.3s ease-in-out;
                }

                .vira-card .vira-card-header .card-icon:after {
                    content: '';
                    height: 100px;
                    width: 100px;
                    background-color: #00dce8;
                    border-radius: 100%;
                    position: absolute;
                    top: 9px;
                    left: 9px;
                }

                .vira-card .vira-card-header .card-icon .fa {
                    margin-top: 45px;
                    font-size: 30px;
                }

                .vira-card .vira-card-header .card-icon span {
                    color: #fff;
                    font-size: 45px;
                    position: relative;
                    z-index: 1;
                }

        .vira-card:hover .vira-card-header .card-icon:after {
            height: 120px;
            width: 120px;
            top: -1px;
            left: -1px;
        }
    </style>

</head>
