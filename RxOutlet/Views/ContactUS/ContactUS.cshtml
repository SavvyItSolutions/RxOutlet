        .title:after {
            content: '';
            height: 5px;
            border: 1px solid #eaeaea;
            width: auto;
            min-width: 100%;
            display: inline-block;
            border-radius: 2px;
        }

        .title .text {
            position: absolute;
            height: 30px;
            background: #fff;
            line-height: 35px;
            font-weight: bold;
            padding-right: 30px;
        }

