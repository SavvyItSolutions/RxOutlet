
<div class="page-content">

    <div class="row">
        <div class="col-sm-3">

        </div>
        <div class="col-sm-6">
            <div class="modal fade in" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: block; padding-right: 17px;">
                <div class="modal-dialog modal-lg" style="width: 50%;left: 5%;">
                    <div class="modal-content">
                        <div class="modal-header">

                            <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">×</span></button>
                            <h2 class="modal-title" id="myModalLabel">For Filling/Transfer Prescription</h2>
                        </div>
                        <div class="modal-body">
                            <p>You don't have complete profile which is required for Filling/Transfer Prescriptions.</p>
                        </div>
                        <div class="modal-footer text-center" style="text-align: center;">
                            <button type="button" class="btn btn-default" data-dismiss="modal"> @Html.ActionLink("Existing Store User", "securityquestionsview" ) </button>
                            <button type="button" class="btn btn-default" data-dismiss="modal"> @Html.ActionLink("Register Online", "Registration") </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-3">

        </div>
    </div>
</div>