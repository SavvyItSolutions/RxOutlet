                                            <button type="submit" class="btn btn-primary btn-lg">Submit</button>
                                        </div>
                                    </div>


                                </div>
                                <div class="vertical-divider">or</div>
                                <div class="col-xs-6">



                                    <form class="form-horizontal" action="" method="post">
                                        <fieldset>


                                            <!-- Name input-->
                                            <div class="form-group">
                                                <label class="col-md-3 control-label" for="name">Physician Name :</label>
                                                <div class="col-md-9">
                                                    <input id="name" name="name" type="text" placeholder="Your name" class="form-control">
                                                </div>
                                            </div>
                                            <br />
                                            <!-- Email input-->
                                            <div class="form-group">
                                                <label class="col-md-3 control-label" for="email"> Physician Number:</label>
                                                <div class="col-md-9">
                                                    <input id="" name="" type="text" placeholder="" class="form-control">
                                                </div>
                                            </div>
                                            <br/>
                                            <!-- Message body -->
                                            <div class="form-group">
                                                <label class="col-md-3 control-label" for="message"> Medication for:</label>
                                                <div class="col-md-9">
                                                    <textarea class="form-control" id="message" name="message" placeholder="Please enter your message here..." rows="5"></textarea>
                                                </div>
                                            </div>

                                            <!-- Form actions -->
                                            <div class="form-group">
                                                <div class="col-md-12 text-right">
                                                    <button type="submit" class="btn btn-primary btn-lg">Submit</button>
                                                </div>
                                            </div>
                                        </fieldset>
                                    </form>


                                </div>
